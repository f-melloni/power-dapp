@using PowerDapp_UserArea.Tools

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PowerDapp_UserArea</title>
    <link rel="stylesheet" href="@Tools.GetFileVersion("~/css/screen.min.css")" />
</head>
<body>
    <div class="container">
        <div class="page-area">
            <figure class="text-center"><img src="~/images/PowerDapp_logo.png" width="420" height="95" title="PowerDapp" /></figure>
            @RenderBody()
        </div>
    </div>
    <script type="text/javascript" src="@Tools.GetFileVersion("~/js/site.min.js")"></script>
    @RenderSection("Scripts", false)
</body>
</html>
